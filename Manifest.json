{
  "manifest_version": 2,

  "name": "NuggetContainer",
  "version": "0.1.1",
  "description": "Because you can't say 'head' or 'box'",
  "icons": { "16": "images/breadsticks16.png",
           "48": "images/breadsticks48.png",
          "128": "images/breadsticks128.png" },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at":"document_end"

  }
]

}
